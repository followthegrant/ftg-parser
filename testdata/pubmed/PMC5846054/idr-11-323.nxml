<article xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:mml="http://www.w3.org/1998/Math/MathML" article-type="research-article"><?properties open_access?><front><journal-meta><journal-id journal-id-type="nlm-ta">Infect Drug Resist</journal-id><journal-id journal-id-type="iso-abbrev">Infect Drug Resist</journal-id><journal-id journal-id-type="publisher-id">Infection and Drug Resistance</journal-id><journal-title-group><journal-title>Infection and Drug Resistance</journal-title></journal-title-group><issn pub-type="epub">1178-6973</issn><publisher><publisher-name>Dove Medical Press</publisher-name></publisher></journal-meta><article-meta><article-id pub-id-type="pmid">29563813</article-id><article-id pub-id-type="pmc">5846054</article-id><article-id pub-id-type="doi">10.2147/IDR.S156534</article-id><article-id pub-id-type="publisher-id">idr-11-323</article-id><article-categories><subj-group subj-group-type="heading"><subject>Original Research</subject></subj-group></article-categories><title-group><article-title>Clustering and recent transmission of <italic>Mycobacterium tuberculosis</italic> in a Chinese population</article-title></title-group><contrib-group><contrib contrib-type="author" equal-contrib="yes"><name><surname>Xu</surname><given-names>Guisheng</given-names></name><xref ref-type="aff" rid="af1-idr-11-323">1</xref><xref ref-type="author-notes" rid="fn1-idr-11-323">*</xref></contrib><contrib contrib-type="author" equal-contrib="yes"><name><surname>Mao</surname><given-names>Xuhua</given-names></name><xref ref-type="aff" rid="af2-idr-11-323">2</xref><xref ref-type="author-notes" rid="fn1-idr-11-323">*</xref></contrib><contrib contrib-type="author"><name><surname>Wang</surname><given-names>Jianming</given-names></name><xref ref-type="aff" rid="af1-idr-11-323">1</xref><xref ref-type="corresp" rid="c1-idr-11-323"/></contrib><contrib contrib-type="author"><name><surname>Pan</surname><given-names>Hongqiu</given-names></name><xref ref-type="aff" rid="af3-idr-11-323">3</xref><xref ref-type="corresp" rid="c2-idr-11-323"/></contrib></contrib-group><aff id="af1-idr-11-323">
