hor-3" contrib-type="author"><name><surname>Chua</surname><given-names>Eng Guan</given-names></name><xref ref-type="aff" rid="aff-3">3</xref></contrib><contrib id="author-4" contrib-type="author"><name><surname>He</surname><given-names>Yongsheng</given-names></name><xref ref-type="aff" rid="aff-2">2</xref></contrib><contrib id="author-5" contrib-type="author"><name><surname>Shu</surname><given-names>Qing</given-names></name><xref ref-type="aff" rid="aff-1">1</xref></contrib><contrib id="author-6" contrib-type="author"><name><surname>Zeng</surname><given-names>Li</given-names></name><xref ref-type="aff" rid="aff-1">1</xref></contrib><contrib id="author-7" contrib-type="author"><name><surname>Luo</surname><given-names>Shiyang</given-names></name><xref ref-type="aff" rid="aff-2">2</xref></contrib><contrib id="author-8" contrib-type="author"><name><surname>Marshall</surname><given-names>Barry J.</given-names></name><xref ref-type="aff" rid="aff-2">2</xref><xref ref-type="aff" rid="aff-3">3</xref></contrib><contrib id="author-9" contrib-type="author" corresp="yes"><name><surname>Liu</surname><given-names>Aijun</given-names></name><xref ref-type="aff" rid="aff-2">2</xref><email>liuaijunsz@163.com</email></contrib><contrib id="author-10" contrib-type="author" corresp="yes"><name><surname>Tay</surname><given-names>Chin Yen</given-names></name><xref ref-type="aff" rid="aff-2">2</xref><xref ref-type="aff" rid="aff-3">3</xref><email>alfred.tay@uwa.edu.au</email></contrib><aff id="aff-1">
