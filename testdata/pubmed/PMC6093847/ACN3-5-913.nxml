</xref></contrib><contrib id="acn3579-cr-0009" contrib-type="author"><collab collab-type="authors">the DEMAND V study group<contrib-group><contrib id="acn3579-cr-0010" contrib-type="author"><name><surname>Iannaccone</surname><given-names>Susan T.</given-names></name><xref ref-type="contrib" rid="acn3579-cr-0009"/></contrib><contrib id="acn3579-cr-0011" contrib-type="author"><name><surname>Karachunski</surname><given-names>Peter I.</given-names></name><xref ref-type="contrib" rid="acn3579-cr-0009"/></contrib><contrib id="acn3579-cr-0012" contrib-type="author"><name><surname>Mathews</surname><given-names>Katherine D.</given-names></name><xref ref-type="contrib" rid="acn3579-cr-0009"/></contrib><contrib id="acn3579-cr-0013" contrib-type="author"><name><surname>Henricson</surname><given-names>Erik K.</given-names></name><xref ref-type="contrib" rid="acn3579-cr-0009"/></contrib><contrib id="acn3579-cr-0014" contrib-type="author"><name><surname>Joyce</surname><given-names>Nanette C.</given-names></name><xref ref-type="contrib" rid="acn3579-cr-0009"/></contrib><contrib id="acn3579-cr-0015" contrib-type="author"><name><surname>Pestronk</surname><given-names>Alan</given-names></name><xref ref-type="contrib" rid="acn3579-cr-0009"/></contrib><contrib id="acn3579-cr-0016" contrib-type="author"><name><surname>Renfoe</surname><given-names>James B.</given-names></name><xref ref-type="contrib" rid="acn3579-cr-0009"/></contrib><contrib id="acn3579-cr-0017" contrib-type="author"><name><surname>Russman</surname><given-names>Barry S.</given-names></name><xref ref-type="contrib" rid="acn3579-cr-0009"/></contrib><contrib id="acn3579-cr-0018" contrib-type="author"><name><surname>Smith</surname><given-names>Edward C.</given-names></name><xref ref-type="contrib" rid="acn3579-cr-0009"/></contrib><contrib id="acn3579-cr-0019" contrib-type="author"><name><surname>So</surname><given-names>Yuen T.</given-names></name><xref ref-type="contrib" rid="acn3579-cr-0009"/></contrib><contrib id="acn3579-