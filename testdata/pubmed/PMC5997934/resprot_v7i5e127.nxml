<article xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:mml="http://www.w3.org/1998/Math/MathML" article-type="research-article"><?properties open_access?><front><journal-meta><journal-id journal-id-type="nlm-ta">JMIR Res Protoc</journal-id><journal-id journal-id-type="iso-abbrev">JMIR Res Protoc</journal-id><journal-id journal-id-type="publisher-id">ResProt</journal-id><journal-title-group><journal-title>JMIR Research Protocols</journal-title></journal-title-group><issn pub-type="epub">1929-0748</issn><publisher><publisher-name>JMIR Publications</publisher-name><publisher-loc>Toronto, Canada</publisher-loc></publisher></journal-meta><article-meta><article-id pub-id-type="pmid">29807883</article-id><article-id pub-id-type="pmc">5997934</article-id><article-id pub-id-type="publisher-id">v7i5e127</article-id><article-id pub-id-type="doi">10.2196/resprot.9431</article-id><article-categories><subj-group subj-group-type="heading"><subject>Protocol</subject></subj-group><subj-group subj-group-type="article-type"><subject>Protocol</subject></subj-group></article-categories><title-group><article-title>A Role for New Brain Magnetic Resonance Imaging Modalities in Daily Clinical Practice: Protocol of the Prediction of Cognitive Recovery After Stroke (PROCRAS) Study</article-title></title-group><contrib-group><contrib contrib-type="editor"><name><surname>Eysenbach</surname><given-names>Gunther</given-names></name></contrib></contrib-group><contrib-group><contrib contrib-type="reviewer"><name><surname>Ballester</surname><given-names>Belen</given-names></name></contrib><contrib contrib-type="reviewer"><name><surname>Pendlebury</surname><given-names>Sarah</given-names></name></contrib></contrib-group><contrib-group><contrib id="contrib1" contrib-type="author" corresp="yes"><name><surname>Aben</surname><given-names>Hugo P</given-names></name><degrees>MD</degrees><on-behalf-of>PROCRAS study group</on-behalf-of><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0003-3032-8843</contrib-id><xref ref-type="aff" rid="aff1">1</xref><xref ref-type="aff" rid="aff2">2</xref><address><institution>Elisabeth Tweesteden Hospital</institution><institution>Department of Neurology</institution><addr-line>PO Box 90151</addr-line><addr-line>Tilburg, 5000 LC</addr-line><country>Netherlands</country><phone>31 135392552</phone><email>h.aben@etz.nl</email></address></contrib><contrib id="contrib2" contrib-type="author"><name><surname>Reijmer</surname><given-names>Yael D</given-names></name><degrees>PhD</degrees><xref ref-type="aff" rid="aff2">2</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0002-2566-7122</contrib-id></contrib><contrib id="contrib3" contrib-type="author"><name><surname>Visser-Meily</surname><given-names>Johanna MA</given-names></name><degrees>MD, PhD</degrees><xref ref-type="aff" rid="aff3">3</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0002-5955-8012</contrib-id></contrib><contrib id="contrib4" contrib-type="author"><name><surname>Spikman</surname><given-names>Jacoba M</given-names></name><degrees>PhD</degrees><xref ref-type="aff" rid="aff4">4</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0002-6477-0763</contrib-id></contrib><contrib id="contrib5" contrib-type="author"><name><surname>de Bresser</surname><given-names>Jeroen</given-names></name><degrees>MD, PhD</degrees><xref ref-type="aff" rid="aff5">5</xref><xref ref-type="aff" rid="aff6">6</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0003-0759-8407</contrib-id></contrib><contrib id="contrib6" contrib-type="author"><name><surname>Biessels</surname><given-names>Geert Jan</given-names></name><degrees>MD, PhD</degrees><xref ref-type="aff" rid="aff2">2</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0001-6862-2496</contrib-id></contrib><contrib id="contrib7" contrib-type="author"><name><surname>de Kort</surname><given-names>Paul LM</given-names></name><degrees>MD, PhD</degrees><xref ref-type="aff" rid="a