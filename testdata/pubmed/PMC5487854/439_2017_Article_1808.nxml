<article xmlns:mml="http://www.w3.org/1998/Math/MathML" xmlns:xlink="http://www.w3.org/1999/xlink" article-type="research-article"><?properties open_access?><front><journal-meta><journal-id journal-id-type="nlm-ta">Hum Genet</journal-id><journal-id journal-id-type="iso-abbrev">Hum. Genet</journal-id><journal-title-group><journal-title>Human Genetics</journal-title></journal-title-group><issn pub-type="ppub">0340-6717</issn><issn pub-type="epub">1432-1203</issn><publisher><publisher-name>Springer Berlin Heidelberg</publisher-name><publisher-loc>Berlin/Heidelberg</publisher-loc></publisher></journal-meta><article-meta><article-id pub-id-type="pmid">28500464</article-id><article-id pub-id-type="pmc">5487854</article-id><article-id pub-id-type="publisher-id">1808</article-id><article-id pub-id-type="doi">10.1007/s00439-017-1808-5</article-id><article-categories><subj-group subj-group-type="heading"><subject>Original Investigation</subject></subj-group></article-categories><title-group><article-title>Global skin colour prediction from DNA</article-title></title-group><contrib-group><contrib contrib-type="author" corresp="yes"><name><surname>Walsh</surname><given-names>Susan</given-names></name><address><phone>+1-317-274-0593</phone><email>walshsus@iupui.edu</email></address><xref ref-type="aff" rid="Aff1">1</xref></contrib><contrib contrib-type="author"><name><surname>Chaitanya</surname><given-names>Lakshmi</given-names></name><xref ref-type="aff" rid="Aff2">2</xref></contrib><contrib contrib-type="author"><name><surname>Breslin</surname><given-names>Krystal</given-names></name><xref ref-type="aff" rid="Aff1">1</xref></contrib><contrib contrib-type="author"><name><surname>Muralidharan</surname><given-names>Charanya</given-names></name><xref ref-type="aff" rid="Aff1">1</xref></contrib><contrib contrib-type="author"><name><surname>Bronikowska</surname><given-names>Agnieszka</given-names></name><xref ref-type="aff" rid="Aff3">3</xref></contrib><contrib contrib-type="author"