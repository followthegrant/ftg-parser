thor"><name><surname>Mao</surname><given-names>Guangyun</given-names></name><xref ref-type="aff" rid="af4-ndt-14-2665">4</xref></contrib><contrib contrib-type="author"><name><surname>Mao</surname><given-names>Shanping</given-names></name><xref ref-type="aff" rid="af5-ndt-14-2665">5</xref></contrib><contrib contrib-type="author"><name><surname>Lin</surname><given-names>Yan</given-names></name><xref ref-type="aff" rid="af6-ndt-14-2665">6</xref></contrib><contrib contrib-type="author"><name><surname>Shen</surname><given-names>Shanshan</given-names></name><xref ref-type="aff" rid="af7-ndt-14-2665">7</xref></contrib><contrib contrib-type="author"><name><surname>Li</surname><given-names>Changjin</given-names></name><xref ref-type="aff" rid="af8-ndt-14-2665">8</xref></contrib><contrib contrib-type="author"><name><surname>Shi</surname><given-names>Hongying</given-names></name><xref ref-type="aff" rid="af4-ndt-14-2665">4</xref></contrib><contrib contrib-type="author"><name><surname>Jiang</surname><given-names>Suo</given-names></name><xref ref-type="aff" rid="af8-ndt-14-2665">8</xref></contrib><contrib contrib-type="author"><name><surname>He</surname><given-names>Jingjing</given-names></name><xref ref-type="aff" rid="af9-ndt-14-2665">9</xref></contrib><contrib contrib-type="author"><name><surname>Zhao</surname><given-names>Ke</given-names></name><xref ref-type="aff" rid="af8-ndt-14-2665">8</xref></contrib><contrib contrib-type="author"><name><surname>Fu</surname><given-names>Ye</given-names></name><xref ref-type="aff" rid="af10-ndt-14-2665">10</xref></contrib><contrib contrib-type="author"><name><surname>Hu</surname><given-names>Xiaoqu</given-names></name><xref ref-type="aff" rid="af11-ndt-14-2665">11</xref></contrib><contrib contrib-type="author"><name><surname>Gu</surname><given-names>Yingying</given-names></name><xref ref-type="aff" rid="af1-ndt-14-2665">1</xref></contrib><contrib contrib-type="author"><name><surname>Wang</surname><given-names>Kai</given-names></name><xref ref-type="aff" rid="af12-ndt-14-2665">12</xref></contrib><contrib contrib-type="author"><name><surname>Guo</surname><given-names>Xiuchan</given-names></name><xref ref-type="aff" rid="af13-ndt-14-2665">13</xref><xref ref-type="corresp" rid="c2-ndt-14-2665"/></contrib><contrib contrib-type="author"><name><surname>He</surname><given-names>Jincai</given-names></name><xref ref-type="aff" rid="af1-ndt-14-2665">1</xref><xref ref-type="aff" rid="af8-ndt-14-2665">8</xref><xref ref-type="corresp" rid="c1-ndt-14-2665"/></contrib></contrib-group><aff id="af1-ndt-14-2665">
