<article xmlns:xlink="http://www.w3.org/1999/xlink" article-type="research-article"><?properties open_access?><front><journal-meta><journal-id journal-id-type="nlm-ta">BMC Plant Biol</journal-id><journal-title>BMC Plant Biology</journal-title><issn pub-type="epub">1471-2229</issn><publisher><publisher-name>BioMed Central</publisher-name></publisher></journal-meta><article-meta><article-id pub-id-type="pmid">19788737</article-id><article-id pub-id-type="pmc">2760556</article-id><article-id pub-id-type="publisher-id">1471-2229-9-123</article-id><article-id pub-id-type="doi">10.1186/1471-2229-9-123</article-id><article-categories><subj-group subj-group-type="heading"><subject>Database</subject></subj-group></article-categories><title-group><article-title>MoccaDB - an integrative database for functional, comparative and diversity studies in the <italic>Rubiaceae </italic>family</article-title></title-group><contrib-group><contrib id="A1" contrib-type="author"><name><surname>Plechakova</surname><given-names>Olga</given-names></name><xref ref-type="aff" rid="I1">1</xref><email>olga.plechakova@ird.fr</email></contrib><contrib id="A2" contrib-type="author"><name><surname>Tranchant-Dubreuil</surname><given-names>Christine</given-names></name><xref ref-type="aff" rid="I1">1</xref><email>christine.tranchant@ird.fr</email></contrib><contrib id="A3" contrib-type="author"><name><surname>Benedet</surname><given-names>Fabrice</given-names></name><xref ref-type="aff" rid="I1">1</xref><xref ref-type="aff" rid="I2">2</xref><email>fabrice.benedet@cirad.fr</email></contrib><contrib id="A4" contrib-type="author"><name><surname>Couderc</surname><given-names>Marie</given-names></name><xref ref-type="aff" rid="I1">1</xref><email>marie.couderc@ird.fr</email></contrib><contrib id="A5" contrib-type="author"><name><surname>Tinaut</surname><given-names>Alexandra</given-names></name><xref ref-type="aff" rid="I1">1</xref><email>tinautalexandra@hotmail.com</email></contrib><contrib id="A6" contrib-type="author"><name><surname>Viader</surname><given-names>V&#x000e9;ronique</given-names></name><xref ref-type="aff" rid="I1">1</xref><xref ref-type="aff" rid="I3">3</xref><email>veronique.viader@supagro.inra.fr</email></contrib><contrib id="A7" contrib-type="author"><name><surname>De Block</surname><given-names>Petra</given-names></name><xref ref-type="aff" rid="I4">4</xref><email>petra.deblock@br.fgov.be</email></contrib><contrib id="A8" contrib-type="author"><name><surname>Hamon</surname><given-names>Perla</given-names></name><xref ref-type="aff" rid="I1">1</xref><email>perla.hamon@ird.fr</email></contrib><contrib id="A9" contrib-type="author"><name><surname>Campa</surname><given-names>Claudine</given-names></name><xref ref-type="aff" rid="I1">1</xref><email>claudine.campa@ird.fr</email></contrib><contrib id="A10" contrib-type="author"><name><surname>de Kochko</surname><given-names>Alexandre</given-names></name><xref ref-type="aff" rid="I1">1</xref><email>alexandre.dekochko@ird.fr</email></contrib><contrib id="A11" contrib-type="author"><name><surname>Hamon</surname><given-names>Serge</given-names></name><xref ref-type="aff" rid="I1">1</xref><email>serge.hamon@ird.fr</email></contrib><contrib id="A12" corresp="yes" contrib-type="author"><name><surname>Poncet</surname><given-names>Val&#x000e9;rie</given-names></name><xref ref-type="aff" rid="I1">1</xref><email>valerie.poncet@ird.fr</email></contrib></contrib-group><aff id="I1"><label>1</label>UMR DIAPC, IRD, 911 avenue Agropolis, BP 64501, 34394 Montpellier Cedex 5, France</aff><aff id="I2"><label>2</label>CIRAD TA C 37/D, Campus International de Baillarguet 34398 Montpellier Cedex 5, France</aff><aff id="I3"><label>3</label>UMR DIAPC, INRA, Domaine de MelgueiI, Chemin de M&#x000e9;zouls, 34130 Mauguio, France</aff><aff id="I4"><label>4</label>National Botanic Garden of Belgium, Domein van Bouchout, 1860 Meise, Belgium</aff><pub-date pub-type="collection"><year>2009</year></pub-date><pub-date pub-type="epub"><day>29</day><month>9</month><year>2009</year></pub-date><volume>9</volume><fpage>123</fpage><lpage>123</lpage><ext-link ext-link-type="uri" xlink:href="http://www.biomedcentral.com/1471-2229/9/123"/><history><date date-type="received"><day>23</day><month>4</month><year>2009</year></date><date date-type="accepted"><day>29</day><month>9</month><year>2009</year></date></history><permissions><copyright-statement>Copyright &#x000a9; 2009 Plechakova et al; licensee BioMed Central Ltd.</copyright-statement><copyright-year>2009</copyright-year><copyright-holder>Plechakova et al; licensee BioMed Central Ltd.</copyright-holder><license license-type="open-access" xlink:href="http://creativecommons.org/licenses/by/2.0"><p>This is an Open Access article distributed under the terms of the Creative Commons Attribution License (<ext-link ext-link-type="uri" xlink:href="http://creativecommons.org/licenses/by/2.0"/>), which permits unrestricted use, distribution, and reproduction in any medium, provided the original work is properly cited.</p><!--<rdf xmlns="http://web.resource.org/cc/" xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:dc="http://purl.org/dc/elements/1.1" xmlns:dcterms="http://purl.org/dc/terms"><Work xmlns:dc="http://purl.org/dc/elements/1.1/" xmlns:dcterms="http://purl.org/dc/terms/" rdf:about=""><license rdf:resource="http://creativecommons.org/licenses/by/2.0"/><dc:type rdf:resource="http://purl.org/dc/dcmitype/Text"/><dc:author>
