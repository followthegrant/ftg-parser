				\setlength{\oddsidemargin}{-69pt}
				\begin{document}$$\begin{aligned} m(t) &#x00026;= \int _0^tg(\varsigma )f(t-\varsigma ){\mathrm {d}}\varsigma = \int _0^\theta g(\varsigma )f(t-\varsigma ){\mathrm {d}}\varsigma \\&#x00026;\quad + \int _{t-\theta }^tg(\varsigma )f(t-\varsigma ){\mathrm {d}}\varsigma +\int _\theta ^{t-\theta } g(\varsigma )f(t-\varsigma ){\mathrm {d}}\varsigma \\ &#x00026;= \int _0^\theta g(\varsigma )f(t-\varsigma ){\mathrm {d}}\varsigma + \int _0^\theta g(t-x)f(x){\mathrm {d}}x +\int _\theta ^{t-\theta } g(\varsigma )f(t-\varsigma ){\mathrm {d}}\varsigma \end{aligned}$$\end{document}</tex-math><mml:math id="M130" display="block"><mml:mrow><mml:mtable columnspacing="0.5ex"><mml:mtr><mml:mtd columnalign="right"><mml:mrow><mml:mi>m</mml:mi><mml:mo stretchy="false">(</mml:mo><mml:mi>t</mml:mi><mml:mo stretchy="false">)</mml:mo></mml:mrow></mml:mtd><mml:mtd columnalign="left"><mml:mrow><mml:mo>=</mml:mo><mml:msubsup><mml:mo>&#x0222b;</mml:mo><mml:mn>0</mml:mn><mml:mi>t</mml:mi></mml:msubsup><mml:mi>g</mml:mi><mml:mrow><mml:mo stretchy="false">(</mml:mo><mml:mi mathvariant="italic">&#x003c2;</mml:mi><mml:mo stretchy="false">)</mml:mo></mml:mrow><mml:mi>f</mml:mi><mml:mrow><mml:mo stretchy="false">(</mml:mo><mml:mi>t</mml:mi><mml:mo>-</mml:mo><mml:mi mathvariant="italic">&#x003c2;</mml:mi><mml:mo stretchy="false">)</mml:mo></mml:mrow><mml:mi mathvariant="normal">d</mml:mi><mml:mi mathvariant="italic">&#x003c2;</mml:mi><mml:mo>=</mml:mo><mml:msubsup><mml:mo>&#x0222b;</mml:mo><mml:mn>0</mml:mn><mml:mi mathvariant="italic">&#x003b8;</mml:mi></mml:msubsup><mml:mi>g</mml:mi><mml:mrow><mml:mo stretchy="false">(</mml:mo><mml:mi mathvariant="italic">&#x003c2;</mml:mi><mml:mo stretchy="false">)</mml:mo></mml:mrow><mml:mi>f</mml:mi><mml:mrow><mml:mo stretchy="false">(</mml:mo><mml:mi>t</mml:mi><mml:mo>-</mml:mo><mml:mi mathvariant="italic">&#x003c2;</mml:mi><mml:mo stretchy="false">)</mml:mo></mml:mrow><mml:mi mathvariant="normal">d</mml:mi><mml:mi mathvariant="italic">&#x003c2;</mml:mi></mml:m