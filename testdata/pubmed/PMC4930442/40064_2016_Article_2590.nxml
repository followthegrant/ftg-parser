				\begin{document}$$\begin{aligned} S_\tau ^\star (t)=\overline{F^\star }(t)+\int _0^{t-\zeta } \overline{F^\star }(t-\varsigma ){\mathrm {d}}N^\star (\varsigma ). \end{aligned}$$\end{document}</tex-math><mml:math id="M366" display="block"><mml:mrow><mml:mtable columnspacing="0.5ex"><mml:mtr><mml:mtd columnalign="right"><mml:mrow><mml:msubsup><mml:mi>S</mml:mi><mml:mi mathvariant="italic">&#x003c4;</mml:mi><mml:mo>&#x022c6;</mml:mo></mml:msubsup><mml:mrow><mml:mo stretchy="false">(</mml:mo><mml:mi>t</mml:mi><mml:mo stretchy="false">)</mml:mo></mml:mrow><mml:mo>=</mml:mo><mml:mover><mml:msup><mml:mi>F</mml:mi><mml:mo>&#x022c6;</mml:mo></mml:msup><mml:mo>&#x000af;</mml:mo></mml:mover><mml:mrow><mml:mo stretchy="false">(</mml:mo><mml:mi>t</mml:mi><mml:mo stretchy="false">)</mml:mo></mml:mrow><mml:mo>+</mml:mo><mml:msubsup><mml:mo>&#x0222b;</mml:mo><mml:mn>0</mml:mn><mml:mrow><mml:mi>t</mml:mi><mml:mo>-</mml:mo><mml:mi mathvariant="italic">&#x003b6;</mml:mi></mml:mrow></mml:msubsup><mml:mover><mml:msup><mml:mi>F</mml:mi><mml:mo>&#x022c6;</mml:mo></mml:msup><mml:mo>&#x000af;</mml:mo></mml:mover><mml:mrow><mml:mo stretchy="false">(</mml:mo><mml:mi>t</mml:mi><mml:mo>-</mml:mo><mml:mi mathvariant="italic">&#x003c2;</mml:mi><mml:mo stretchy="false">)</mml:mo></mml:mrow><mml:mi mathvariant="normal">d</mml:mi><mml:msup><mml:mi>N</mml:mi><mml:mo>&#x022c6;</mml:mo></mml:msup><mml:mrow><mml:mo stretchy="false">(</mml:mo><mml:mi mathvariant="italic">&#x003c2;</mml:mi><mml:mo stretchy="false">)</mml:mo></mml:mrow><mml:mo>.</mml:mo></mml:mrow></mml:mtd></mml:mtr></mml:mtable></mml:mrow></mml:math><graphic xlink:href="40064_2016_2590_Article_Equ13.gif" position="anchor"/></alternatives></disp-formula>Thus<disp-formula id="Equ57"><alternatives><tex-math id="M367">\documentclass[12pt]{minimal}
