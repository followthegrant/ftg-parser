<article xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:mml="http://www.w3.org/1998/Math/MathML" article-type="research-article"><?properties open_access?><front><journal-meta><journal-id journal-id-type="nlm-ta">PLoS Pathog</journal-id><journal-id journal-id-type="iso-abbrev">PLoS Pathog</journal-id><journal-id journal-id-type="publisher-id">ppat</journal-id><journal-id journal-id-type="publisher-id">plpa</journal-id><journal-id journal-id-type="pmc">plospath</journal-id><journal-title-group><journal-title>PLoS Pathogens</journal-title></journal-title-group><issn pub-type="ppub">1553-7366</issn><issn pub-type="epub">1553-7374</issn><publisher><publisher-name>Public Library of Science</publisher-name><publisher-loc>San Francisco, USA</publisher-loc></publisher></journal-meta><article-meta><article-id pub-id-type="pmid">18166078</article-id><article-id pub-id-type="pmc">2156095</article-id><article-id pub-id-type="doi">10.1371/journal.ppat.0030195</article-id><article-id pub-id-type="publisher-id">07-PLPA-RA-0030R3</article-id><article-id pub-id-type="sici">plpa-03-12-19</article-id><article-categories><subj-group subj-group-type="heading"><subject>Research Article</subject></subj-group><subj-group subj-group-type="Discipline"><subject>Infectious Diseases</subject></subj-group><subj-group subj-group-type="System Taxonomy"><subject>Malaria</subject><subject>Mosquito</subject><subject>Dynamics</subject></subj-group></article-categories><title-group><article-title>Progression of <named-content content-type="genus-species">Plasmodium berghei</named-content> through <named-content content-type="genus-species">Anopheles stephensi</named-content> Is Density-Dependent</article-title><alt-title alt-title-type="running-head">Malaria in the Mosquito</alt-title></title-group><contrib-group><contrib contrib-type="author"><name><surname>Sinden</surname><given-names>Robert E</given-names></name><xref ref-type="aff" rid="aff1">1</xref><xref ref-type="corresp" rid="cor1">&#x0002a;</xref></contrib><contrib contrib-type="author"><name><surname>Dawes</surname><given-names>Emma J</given-names></name><xref ref-type="aff" rid="aff2">2</xref></contrib><contrib contrib-type="author"><name><surname>Alavi</surname><given-names>Yasmene</given-names></name><xref ref-type="aff" rid="aff1">1</xref></contrib><contrib contrib-type="author"><name><surname>Waldock</surname><given-names>Joanna</given-names></name><xref ref-type="aff" rid="aff1">1</xref></contrib><contrib contrib-type="author"><name><surname>Finney</surname><given-names>Olivia</given-names></name><xref ref-type="aff" rid="aff1">1</xref></contrib><contrib contrib-type="author"><name><surname>Mendoza</surname><given-names>Jacqui</given-names></name><xref ref-type="aff" rid="aff1">1</xref></contrib><contrib contrib-type="author"><name><surname>Butcher</surname><given-names>Geoff A</given-names></name><xref ref-type="aff" rid="aff1">1</xref></contrib><contrib contrib-type="author"><name><surname>Andrews</surname><given-names>Laura</given-names></name><xref ref-type="aff" rid="aff3">3</xref></contrib><contrib contrib-type="author"><name><surname>Hill</surname><given-names>Adrian V</given-names></name><xref ref-type="aff" rid="aff3">3</xref></contrib><contrib contrib-type="author"><name><surname>Gilbert</surname><given-names>Sarah C</given-names></name><xref ref-type="aff" rid="aff3">3</xref></contrib><contrib contrib-type="author"><name><surname>Bas&#x000e1;&#x000f1;ez</surname><given-names>Mar&#x000ed;a-Gloria</given-names></name><xref ref-type="aff" rid="aff2">2</xref></contrib></contrib-group><aff id="aff1">
