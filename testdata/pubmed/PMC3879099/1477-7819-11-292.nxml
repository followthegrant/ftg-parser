umor grade:<hr/></td><td align="center" valign="bottom">&#x000a0;<hr/></td><td align="center" valign="bottom">&#x000a0;<hr/></td><td align="center" valign="bottom">0.006<hr/></td></tr><tr><td align="center" valign="bottom">G1<hr/></td><td align="center" valign="bottom">3<hr/></td><td align="center" valign="bottom">2<hr/></td><td align="center" valign="bottom">&#x000a0;<hr/></td></tr><tr><td align="center" valign="bottom">G2<hr/></td><td align="center" valign="bottom">44<hr/></td><td align="center" valign="bottom">32<hr/></td><td align="center" valign="bottom">&#x000a0;<hr/></td></tr><tr><td align="center" valign="bottom">G3<hr/></td><td align="center" valign="bottom">9<hr/></td><td align="center" valign="bottom">1<hr/></td><td align="center" valign="bottom">&#x000a0;<hr/></td></tr><tr><td align="center" valign="bottom">Gx<hr/></td><td align="center" valign="bottom">5<hr/></td><td align="center" valign="bottom">15<hr/></td><td align="center" valign="bottom">&#x000a0;<hr/></td></tr><tr><td align="center" valign="bottom">Stage:<hr/></td><td align="center" valign="bottom">&#x000a0;<hr/></td><td align="center" valign="bottom">&#x000a0;<hr/></td><td align="center" valign="bottom">0.06<hr/></td></tr><tr><td align="center" valign="bottom">0<hr/></td><td align="center" valign="bottom">3<hr/></td><td align="center" valign="bottom">4<hr/></td><td align="center" valign="bottom">&#x000a0;<hr/></td></tr><tr><td align="center" valign="bottom">I<hr/></td><td align="center" valign="bottom">20<hr/></td><td align="center" valign="bottom">11<hr/></td><td align="center" valign="bottom">&#x000a0;<hr/></td></tr><tr><td align="center" valign="bottom">II<hr/></td><td align="center" valign="bottom">13<hr/></td><td align="center" valign="bottom">19<hr/></td><td align="center" valign="bottom">&#x000a0;<hr/></td></tr><tr><td align="center" valign="bottom">III<hr/></td><td align="center" valign="bottom">23<hr/></td><td align="center" valign="bottom">6<hr/></td><td align="center" valign="bottom">