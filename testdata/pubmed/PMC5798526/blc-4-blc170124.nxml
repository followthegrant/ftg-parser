<article xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:mml="http://www.w3.org/1998/Math/MathML" article-type="research-article"><?properties open_access?><front><journal-meta><journal-id journal-id-type="nlm-ta">Bladder Cancer</journal-id><journal-id journal-id-type="iso-abbrev">Bladder Cancer</journal-id><journal-id journal-id-type="publisher-id">BLC</journal-id><journal-title-group><journal-title>Bladder Cancer (Amsterdam, Netherlands)</journal-title></journal-title-group><issn pub-type="ppub">2352-3727</issn><issn pub-type="epub">2352-3735</issn><issn-l>2352-3727</issn-l><publisher><publisher-name>IOS Press</publisher-name><publisher-loc>Nieuwe Hemweg 6B, 1013 BG Amsterdam, The Netherlands</publisher-loc></publisher></journal-meta><article-meta><article-id pub-id-type="pmid">29430508</article-id><article-id pub-id-type="pmc">5798526</article-id><article-id pub-id-type="publisher-id">BLC170124</article-id><article-id pub-id-type="doi">10.3233/BLC-170124</article-id><article-categories><subj-group subj-group-type="heading"><subject>Research Report</subject></subj-group></article-categories><title-group><article-title>Decreased Invasion of Urothelial Carcinoma of the Bladder by Inhibition of Matrix-Metalloproteinase 7</article-title><alt-title alt-title-type="left-running-head">C. Bolenz et al.</alt-title><alt-title alt-title-type="right-running-head">MMP7 Promotes Bladder Cancer Cell Invasion</alt-title></title-group><contrib-group><contrib contrib-type="author"><name><surname>Bolenz</surname><given-names>Christian</given-names></name><xref ref-type="aff" rid="aff1">a</xref><xref ref-type="author-notes" rid="fn1">1</xref><xref ref-type="corresp" rid="cor1">*</xref></contrib><contrib contrib-type="author"><name><surname>Knauf</surname><given-names>Daniel</given-names></name><xref ref-type="aff" rid="aff2">b</xref><xref ref-type="author-notes" rid="fn1">1</xref></contrib><contrib contrib-type="author"><name><surname>John</surname><given-names>Axel</given-names></name><xref ref-type="aff" rid="aff1">a</xref></contrib><contrib contrib-type="author"><name><surname>Erben</surname><given-names>Philipp</given-names></name><xref ref-type="aff" rid="aff2">b</xref></contrib><contrib contrib-type="author"><name><surname>Steidler</surname><given-names>Annette</given-names></name><xref ref-type="aff" rid="aff2">b</xref></contrib><contrib contrib-type="author"><name><surname>Schneider</surname><given-names>Stefan W.</given-names></name><xref ref-type="aff" rid="aff4">d</xref></contrib><contrib contrib-type="author"><name><surname>G&#x000fc;nes</surname><given-names>Cagatay</given-names></name><xref ref-type="aff" rid="aff1">a</xref></contrib><contrib contrib-type="author"><name><surname>Gorzelanny</surname><given-names>Christian</given-names></name><xref ref-type="aff" rid="aff3">c</xref><xref ref-type="aff" rid="aff4">d</xref><xref ref-type="corresp" rid="cor1">*</xref></contrib><aff id="aff1">
