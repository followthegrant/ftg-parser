<article xmlns:mml="http://www.w3.org/1998/Math/MathML" xmlns:xlink="http://www.w3.org/1999/xlink" article-type="research-article"><?properties open_access?><front><journal-meta><journal-id journal-id-type="nlm-ta">BMC Med Genet</journal-id><journal-id journal-id-type="iso-abbrev">BMC Med. Genet</journal-id><journal-title-group><journal-title>BMC Medical Genetics</journal-title></journal-title-group><issn pub-type="epub">1471-2350</issn><publisher><publisher-name>BioMed Central</publisher-name><publisher-loc>London</publisher-loc></publisher></journal-meta><article-meta><article-id pub-id-type="pmid">28449647</article-id><article-id pub-id-type="pmc">5408434</article-id><article-id pub-id-type="publisher-id">411</article-id><article-id pub-id-type="doi">10.1186/s12881-017-0411-x</article-id><article-categories><subj-group subj-group-type="heading"><subject>Research Article</subject></subj-group></article-categories><title-group><article-title><italic>PEAR1</italic> is not a major susceptibility gene for cardiovascular disease in a Flemish population</article-title></title-group><contrib-group><contrib contrib-type="author"><name><surname>Yang</surname><given-names>Wen-Yi</given-names></name><address><email>wenyi.yang@med.kuleuven.be</email></address><xref ref-type="aff" rid="Aff1">1</xref></contrib><contrib contrib-type="author"><name><surname>Petit</surname><given-names>Thibault</given-names></name><address><email>thibault.petit@uzleuven.be</email></address><xref ref-type="aff" rid="Aff1">1</xref><xref ref-type="aff" rid="Aff2">2</xref></contrib><contrib contrib-type="author"><name><surname>Cauwenberghs</surname><given-names>Nicholas</given-names></name><address><email>nicholas.cauwenberghs@med.kuleuven.be</email></address><xref ref-type="aff" rid="Aff1">1</xref></contrib><contrib contrib-type="author"><name><surname>Zhang</surname><given-names>Zhen-Yu</given-names></name><address><email>zhenyu.yang@med.kuleuven.be</email></address><xref ref-type="aff" rid="Aff1">1</xref></contrib><contrib contrib-type="author"><name><surname>Sheng</surname><given-names>Chang-Sheng</given-names></name><address><email>scsheng2004@163.com</email></address><xref ref-type="aff" rid="Aff1">1</xref></contrib><contrib contrib-type="author"><name><surname>Thijs</surname><given-names>Lutgarde</given-names></name><address><email>lutgarde.thijs@med.kuleuven.be</email></address><xref ref-type="aff" rid="Aff1">1</xref></contrib><contrib contrib-type="author"><name><surname>Salvi</surname><given-names>Erika</given-names></name><address><email>erika.salvi@unimi.it</email></address><xref ref-type="aff" rid="Aff3">3</xref></contrib><contrib contrib-type="author"><name><surname>Izzi</surname><given-names>Benedetta</given-names></name><address><email>benedetta.izzi@med.kuleuven.be</email></address><xref ref-type="aff" rid="Aff4">4</xref></contrib><contrib contrib-type="author"><name><surname>Vandenbriele</surname><given-names>Christophe</given-names></name><address><email>christophe.vandenbriele@med.kuleuven.be</email></address><xref ref-type="aff" rid="Aff4">4</xref></contrib><contrib contrib-type="author"><name><surname>Wei</surname><given-names>Fang-Fei</given-names></name><address><email>fangfei.wei@med.kuleuven.be</email></address><xref ref-type="aff" rid="Aff1">1</xref></contrib><contrib contrib-type="author"><name><surname>Gu</surname><given-names>Yu-Mei</given-names></name><address><email>yumei.gu@med.kuleuven.be</email></address><xref ref-type="aff" rid="Aff1">1</xref></contrib><contrib contrib-type="author"><name><surname>Jacobs</surname><given-names>Lotte</given-names></name><address><email>lotte.jacobs@med.kuleuven.be</email></address><xref ref-type="aff" rid="Aff1">1</xref></contrib><contrib contrib-type="author"><name><surname>Citterio</surname><given-names>Lorena</given-names></name><address><email>citterio.lorena@hsr.it</email></address><xref ref-type="aff" rid="Aff5">5</xref></contrib><contrib contrib-type="author"><name><surname>Delli Carpini<