><xref ref-type="aff" rid="aff2">2</xref><contrib-id contrib-id-type="orcid">https://orcid.org/0000-0002-9766-2769</contrib-id></contrib><contrib id="contrib2" contrib-type="author"><name><surname>Gong</surname><given-names>Dawei</given-names></name><degrees>MM</degrees><xref ref-type="aff" rid="aff3">3</xref><contrib-id contrib-id-type="orcid">https://orcid.org/0000-0002-5020-0508</contrib-id></contrib><contrib id="contrib3" contrib-type="author"><name><surname>Luo</surname><given-names>Huichun</given-names></name><degrees>PhD</degrees><xref ref-type="aff" rid="aff1">1</xref><contrib-id contrib-id-type="orcid">https://orcid.org/0000-0002-7833-1358</contrib-id></contrib><contrib id="contrib4" contrib-type="author"><name><surname>Zhang</surname><given-names>Wenbin</given-names></name><degrees>MD</degrees><xref ref-type="aff" rid="aff4">4</xref><contrib-id contrib-id-type="orcid">https://orcid.org/0000-0002-1024-6165</contrib-id></contrib><contrib id="contrib5" contrib-type="author"><name><surname>Zhang</surname><given-names>Lei</given-names></name><degrees>MSc</degrees><xref ref-type="aff" rid="aff5">5</xref><contrib-id contrib-id-type="orcid">https://orcid.org/0000-0002-6378-1057</contrib-id></contrib><contrib id="contrib6" contrib-type="author"><name><surname>Zhang</surname><given-names>Han</given-names></name><degrees>MEng</degrees><xref ref-type="aff" rid="aff1">1</xref><xref ref-type="aff" rid="aff2">2</xref><contrib-id contrib-id-type="orcid">https://orcid.org/0000-0003-4623-8623</contrib-id></contrib><contrib id="contrib7" contrib-type="author" equal-contrib="yes"><name><surname>Zhou</surname><given-names>Junhong</given-names></name><degrees>PhD</degrees><xref ref-type="aff" rid="aff6">6</xref><contrib-id contrib-id-type="orcid">https://orcid.org/0000-0002-7931-7646</contrib-id></contrib><contrib id="contrib8" contrib-type="author" corresp="yes" equal-contrib="yes"><name><surname>Wang</surname><given-names>Shouyan</given-names></name><degrees>PhD</degrees><con