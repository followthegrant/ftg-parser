<article xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:mml="http://www.w3.org/1998/Math/MathML" article-type="research-article"><?properties open_access?><front><journal-meta><journal-id journal-id-type="nlm-ta">J Infect Dis</journal-id><journal-id journal-id-type="iso-abbrev">J. Infect. Dis</journal-id><journal-id journal-id-type="publisher-id">jid</journal-id><journal-id journal-id-type="hwp">jinfdis</journal-id><journal-title-group><journal-title>The Journal of Infectious Diseases</journal-title></journal-title-group><issn pub-type="ppub">0022-1899</issn><issn pub-type="epub">1537-6613</issn><publisher><publisher-name>Oxford University Press</publisher-name></publisher></journal-meta><article-meta><article-id pub-id-type="pmid">24446523</article-id><article-id pub-id-type="pmc">4091579</article-id><article-id pub-id-type="doi">10.1093/infdis/jiu051</article-id><article-id pub-id-type="publisher-id">jiu051</article-id><article-categories><subj-group subj-group-type="heading"><subject>Major Articles and Brief Reports</subject><subj-group subj-group-type="heading"><subject>HIV/AIDS</subject></subj-group></subj-group></article-categories><title-group><article-title>Dolutegravir in Antiretroviral-Experienced Patients With Raltegravir- and/or Elvitegravir-Resistant HIV-1: 24-Week Results of the Phase III VIKING-3 Study</article-title></title-group><contrib-group><contrib contrib-type="author"><name><surname>Castagna</surname><given-names>Antonella</given-names></name><xref ref-type="aff" rid="af1">1</xref></contrib><contrib contrib-type="author"><name><surname>Maggiolo</surname><given-names>Franco</given-names></name><xref ref-type="aff" rid="af2">2</xref></contrib><contrib contrib-type="author"><name><surname>Penco</surname><given-names>Giovanni</given-names></name><xref ref-type="aff" rid="af3">3</xref></contrib><contrib contrib-type="author"><name><surname>Wright</surname><given-names>David</given-names></name><xref ref-type="aff" rid="af4">4</xref></contrib><contrib contrib-type="author"><name><surname>Mills</surname><given-names>Anthony</given-names></name><xref ref-type="aff" rid="af5">5</xref></contrib><contrib contrib-type="author"><name><surname>Grossberg</surname><given-names>Robert</given-names></name><xref ref-type="aff" rid="af6">6</xref></contrib><contrib contrib-type="author"><name><surname>Molina</surname><given-names>Jean-Michel</given-names></name><xref ref-type="aff" rid="af7">7</xref></contrib><contrib contrib-type="author"><name><surname>Chas</surname><given-names>Julie</given-names></name><xref ref-type="aff" rid="af8">8</xref></contrib><contrib contrib-type="author"><name><surname>Durant</surname><given-names>Jacques</given-names></name><xref ref-type="aff" rid="af9">9</xref></contrib><contrib contrib-type="author"><name><surname>Moreno</surname><given-names>Santiago</given-names></name><xref ref-type="aff" rid="af10">10</xref></contrib><contrib contrib-type="author"><name><surname>Doroana</surname><given-names>Manuela</given-names></name><xref ref-type="aff" rid="af11">11</xref></contrib><contrib contrib-type="author"><name><surname>Ait-Khaled</surname><given-names>Mounir</given-names></name><xref ref-type="aff" rid="af12">12</xref></contrib><contrib contrib-type="author"><name><surname>Huang</surname><given-names>Jenny</given-names></name><xref ref-type="aff" rid="af13">13</xref></contrib><contrib contrib-type="author"><name><surname>Min</surname><given-names>Sherene</given-names></name><xref ref-type="aff" rid="af14">14</xref></contrib><contrib contrib-type="author"><name><surname>Song</surname><given-names>Ivy</given-names></name><xref ref-type="aff" rid="af14">14</xref></contrib><contrib contrib-type="author"><name><surname>Vavro</surname><given-names>Cindy</given-names></name><xref ref-type="aff" rid="af14">14</xref></contrib><contrib contrib-type="author"><name><surname>Nichols</surname><given-names>Garrett</given-names></name><xref ref-type="aff" rid="af14">14</xref></contrib><contrib contrib-typ