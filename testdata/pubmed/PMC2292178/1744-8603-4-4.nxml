<article xmlns:xlink="http://www.w3.org/1999/xlink" article-type="review-article"><?properties open_access?><front><journal-meta><journal-id journal-id-type="nlm-ta">Global Health</journal-id><journal-title>Globalization and Health</journal-title><issn pub-type="epub">1744-8603</issn><publisher><publisher-name>BioMed Central</publisher-name></publisher></journal-meta><article-meta><article-id pub-id-type="pmid">18302739</article-id><article-id pub-id-type="pmc">2292178</article-id><article-id pub-id-type="publisher-id">1744-8603-4-4</article-id><article-id pub-id-type="doi">10.1186/1744-8603-4-4</article-id><article-categories><subj-group subj-group-type="heading"><subject>Review</subject></subj-group></article-categories><title-group><article-title>Convergence of obesity and high glycemic diet on compounding diabetes and cardiovascular risks in modernizing China: An emerging public health dilemma</article-title></title-group><contrib-group><contrib id="A1" corresp="yes" contrib-type="author"><name><surname>Ding</surname><given-names>Eric L</given-names></name><xref ref-type="aff" rid="I1">1</xref><email>eding@post.harvard.edu</email></contrib><contrib id="A2" contrib-type="author"><name><surname>Malik</surname><given-names>Vasanti S</given-names></name><xref ref-type="aff" rid="I1">1</xref><email>vmalik@hsph.harvard.edu</email></contrib></contrib-group><aff id="I1"><label>1</label>Department of Nutrition, Harvard School of Public Health, Boston, Massachusetts, USA</aff><pub-date pub-type="collection"><year>2008</year></pub-date><pub-date pub-type="epub"><day>26</day><month>2</month><year>2008</year></pub-date><volume>4</volume><fpage>4</fpage><lpage>4</lpage><ext-link ext-link-type="uri" xlink:href="http://www.globalizationandhealth.com/content/4/1/4"/><history><date date-type="received"><day>3</day><month>8</month><year>2007</year></date><date date-type="accepted"><day>26</day><month>2</month><year>2008</year></date></history><permissions><copyright-statement>Copyright &#x000a9; 2008 Ding and Malik; licensee BioMed Central Ltd.</copyright-statement><copyright-year>2008</copyright-year><copyright-holder>Ding and Malik; licensee BioMed Central Ltd.</copyright-holder><license license-type="open-access" xlink:href="http://creativecommons.org/licenses/by/2.0"><p>This is an Open Access article distributed under the terms of the Creative Commons Attribution License (<ext-link ext-link-type="uri" xlink:href="http://creativecommons.org/licenses/by/2.0"/>), which permits unrestricted use, distribution, and reproduction in any medium, provided the original work is properly cited.</p><!--<rdf xmlns="http://web.resource.org/cc/" xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:dc="http://purl.org/dc/elements/1.1" xmlns:dcterms="http://purl.org/dc/terms"><Work xmlns:dc="http://purl.org/dc/elements/1.1/" xmlns:dcterms="http://purl.org/dc/terms/" rdf:about=""><license rdf:resource="http://creativecommons.org/licenses/by/2.0"/><dc:type rdf:resource="http://purl.org/dc/dcmitype/Text"/><dc:author>
