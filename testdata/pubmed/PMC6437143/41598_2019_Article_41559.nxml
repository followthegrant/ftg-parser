				\usepackage{upgreek}
				\setlength{\oddsidemargin}{-69pt}
				\begin{document}$${\hat{y}}_{T,m}={\hat{\beta }}_{0,m}+{{\bf{X}}}_{T,m}^{T}{\hat{{\boldsymbol{\beta }}}}_{m},$$\end{document}</tex-math><mml:math id="M2" display="block"><mml:msub><mml:mrow><mml:mrow><mml:mover><mml:mi>y</mml:mi><mml:mo stretchy="false">&#x002c6;</mml:mo></mml:mover></mml:mrow></mml:mrow><mml:mrow><mml:mi>T</mml:mi><mml:mo>,</mml:mo><mml:mi>m</mml:mi></mml:mrow></mml:msub><mml:mo>=</mml:mo><mml:msub><mml:mrow><mml:mrow><mml:mover><mml:mi>&#x003b2;</mml:mi><mml:mo stretchy="false">&#x002c6;</mml:mo></mml:mover></mml:mrow></mml:mrow><mml:mrow><mml:mn>0</mml:mn><mml:mo>,</mml:mo><mml:mi>m</mml:mi></mml:mrow></mml:msub><mml:mo>+</mml:mo><mml:msubsup><mml:mrow><mml:mrow><mml:mrow><mml:mi mathvariant="bold">X</mml:mi></mml:mrow></mml:mrow></mml:mrow><mml:mrow><mml:mi>T</mml:mi><mml:mo>,</mml:mo><mml:mi>m</mml:mi></mml:mrow><mml:mrow><mml:mi>T</mml:mi></mml:mrow></mml:msubsup><mml:msub><mml:mrow><mml:mrow><mml:mover><mml:mrow><mml:mi mathvariant="bold-italic">&#x003b2;</mml:mi></mml:mrow><mml:mo stretchy="false">&#x002c6;</mml:mo></mml:mover></mml:mrow></mml:mrow><mml:mrow><mml:mi>m</mml:mi></mml:mrow></mml:msub><mml:mo>,</mml:mo></mml:math><graphic xlink:href="41598_2019_41559_Article_Equa.gif" position="anchor"/></alternatives></disp-formula>where the vector <bold>X</bold><sub><italic>T</italic>,<italic>m</italic></sub> = (<italic>x</italic><sub><italic>i</italic>,<italic>T</italic>,<italic>m</italic></sub>), and <inline-formula id="IEq1"><alternatives><tex-math id="M3">\documentclass[12pt]{minimal}
