<article xmlns:ali="http://www.niso.org/schemas/ali/1.0" xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:mml="http://www.w3.org/1998/Math/MathML" article-type="research-article"><?properties open_access?><front><journal-meta><journal-id journal-id-type="nlm-ta">Int J Mol Sci</journal-id><journal-id journal-id-type="iso-abbrev">Int J Mol Sci</journal-id><journal-id journal-id-type="publisher-id">ijms</journal-id><journal-title-group><journal-title>International Journal of Molecular Sciences</journal-title></journal-title-group><issn pub-type="epub">1422-0067</issn><publisher><publisher-name>MDPI</publisher-name></publisher></journal-meta><article-meta><article-id pub-id-type="pmid">25809606</article-id><article-id pub-id-type="pmc">4394533</article-id><article-id pub-id-type="doi">10.3390/ijms16036298</article-id><article-id pub-id-type="publisher-id">ijms-16-06298</article-id><article-categories><subj-group subj-group-type="heading"><subject>Article</subject></subj-group></article-categories><title-group><article-title>Genotyping Test with Clinical Factors: Better Management of Acute Postoperative Pain?</article-title></title-group><contrib-group><contrib contrib-type="author"><name><surname>Hajj</surname><given-names>Aline</given-names></name><xref ref-type="aff" rid="af1-ijms-16-06298">1</xref><xref ref-type="aff" rid="af2-ijms-16-06298">2</xref><xref rid="c1-ijms-16-06298" ref-type="corresp">*</xref></contrib><contrib contrib-type="author"><name><surname>Peoc&#x02019;h</surname><given-names>Katell</given-names></name><xref ref-type="aff" rid="af2-ijms-16-06298">2</xref></contrib><contrib contrib-type="author"><name><surname>Laplanche</surname><given-names>Jean-Louis</given-names></name><xref ref-type="aff" rid="af2-ijms-16-06298">2</xref></contrib><contrib contrib-type="author"><name><surname>Jabbour</surname><given-names>Hicham</given-names></name><xref ref-type="aff" rid="af3-ijms-16-06298">3</xref></contrib><contrib contrib-type="author"><name><surname>Naccac